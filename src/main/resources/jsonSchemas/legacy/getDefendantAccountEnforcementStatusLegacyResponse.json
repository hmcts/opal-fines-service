{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Get Defendant Account Enforcement Status Legacy Response",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "version": {
      "type": "integer"
    },
    "enforcement_overview": {
      "$ref": "#/$defs/enforcementOverview"
    },
    "enforcement_override": {
      "$ref": "./common-objects/enforcementOverrideLegacy.json"
    },
    "last_enforcement_action": {
      "$ref": "#/$defs/enforcementOverview"
    },
    "next_enforcement_action_data": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^[A-Za-z0-9]{1,6}(,[A-Za-z0-9]{1,6})*$"
        },
        {
          "type": "string",
          "const": "all"
        },
        {
          "type": "null"
        }
      ]
    },
    "account_status": {
      "$ref": "./common-objects/accountStatusReferenceLegacy.json"
    },
    "employer_flag": {
      "type": "boolean"
    }
  },
  "required": [
    "version",
    "employer_flag",
    "account_status",
    "enforcement_overview"
  ],
  "$defs": {
    "enforcementOverview": {
      "type": "object",
      "properties": {
        "days_in_default": {
          "type": "integer"
        },
        "enforcement_court": {
          "$ref": "./common-objects/courtReferenceLegacy.json"
        },
        "collection_order": {
          "$ref": "./common-objects/collectionOrderLegacy.json"
        }
      },
      "required": [
        "collection_order",
        "enforcement_court"
      ]
    },
    "enforcement_action": {
      "type": "object",
      "properties": {
        "result_reference": {
          "$ref": "./common-objects/resultReferenceLegacy.json"
        },
        "reason": {
          "type": "string"
        },
        "enforcer": {
          "$ref": "./common-objects/enforcerReferenceLegacy.json"
        },
        "warrant_number": {
          "type": "string"
        },
        "date_added": {
          "type": "string",
          "format": "date"
        },
        "result_responses": {
          "type": "array",
          "items": {
            "$ref": "./common-objects/resultResponsesLegacy.json"
          }
        }
      },
      "required": [
        "result_reference",
        "date_added"
      ]
    }
  }
}


