{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "Add Defendant Account Payment Card Request Legacy Response",
  "additionalProperties": false,
  "properties": {
    "defendant_account_id": {
      "type": "string",
      "minLength": 1
    },
    "version": {
      "type": "integer"
    },
    "payment_card_requested": {
      "type": "boolean"
    },
    "payment_card_last_requested": {
      "type": "string",
      "format": "date"
    },
    "payment_card_requested_by": {
      "type": "string",
      "minLength": 1,
      "maxLength": 20
    }
  },
  "required": ["defendant_account_id", "version"]
}
