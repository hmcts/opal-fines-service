{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "Get Defendant Account Payment Terms Legacy Response",
  "additionalProperties": false,
  "properties": {
    "version": {
      "type": "integer"
    },
    "payment_terms" : {
      "$ref": "./common-objects/paymentTermsLegacy.json"
    },
    "posted_details": {
      "$ref": "./common-objects/postedDetailsLegacy.json"
    },
    "payment_card_last_requested": {
      "type": "string",
      "format": "date"
    },
    "extension": {
      "type": "boolean"
    },
    "last_enforcement": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": ["version", "payment_terms", "posted_details"]
}

