{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "Get Defendant Account Payment Terms Legacy Response",
  "additionalProperties": false,
  "properties": {
    "version": {
      "type": "integer"
    },
    "payment_terms_summary" : {
      "$ref": "./common-objects/paymentTermsSummaryLegacy.json"
    },
    "days_in_default": {
      "type": "integer",
      "minimum": 0
    },
    "date_days_in_default_imposed": {
      "type": "string",
      "format": "date"
    },
    "payment_card_last_requested": {
      "type": "string",
      "format": "date"
    },
    "date_last_amended": {
      "type": "string",
      "format": "date"
    },
    "last_amended_by": {
      "type": "string",
      "minLength": 1
    },
    "last_amended_by_id": {
      "type": "integer"
    },
    "amendment_reason": {
      "type": "string",
      "minLength": 1
    },
    "extension": {
      "type": "boolean"
    },
    "last_enforcement": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": ["version", "payment_terms_summary"]
}

