{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "Add Defendant Account Payment Terms Legacy Response",
  "additionalProperties": false,
  "properties": {
    "defendant_account_id": {
      "type": "string",
      "minLength": 1
    },
    "version": {
      "type": "integer"
    },
    "payment_terms": {
      "$ref": "./common-objects/paymentTermsLegacy.json"
    },
    "payment_card_last_requested": {
      "type": "string",
      "format": "date"
    },
    "last_enforcement": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": ["defendant_account_id", "version", "payment_terms"]
}
