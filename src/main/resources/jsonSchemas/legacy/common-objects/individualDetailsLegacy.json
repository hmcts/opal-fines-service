{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Individual Details Legacy",
  "type": "object",
  "properties": {
    "title": { "type": "string" },
    "forenames": { "type": "string" },
    "surname": { "type": "string" },
    "date_of_birth": { "type": "string" },
    "age": { "type": "string" },
    "national_insurance_number": { "type": "string" },
    "individual_aliases": {
      "type": "array",
      "items": { "$ref": "./individualAliasLegacy.json" }
    }
  },
  "required": ["title", "surname"]
}
