{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Defendant Account Party Legacy",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "defendant_account_party_id": {
      "type": "string",
      "minLength": 1
    },
    "defendant_account_party_type": {
      "type": "string",
      "enum": ["Defendant", "Parent/Guardian"]
    },
    "is_debtor": {
      "type": "boolean"
    },
    "party_details": {
      "$ref": "./partyDetailsLegacy.json"
    },
    "address": {
      "$ref": "./addressDetailsLegacy.json"
    },
    "contact_details": {
      "$ref": "./contactDetailsLegacy.json"
    },
    "vehicle_details": {
      "$ref": "./vehicleDetailsLegacy.json"
    },
    "employer_details": {
      "$ref": "./employerDetailsLegacy.json"
    },
    "language_preferences": {
      "$ref": "./languagePreferencesLegacy.json"
    }
  },
  "required": [
    "defendant_account_party_type",
    "is_debtor",
    "party_details",
    "address"
  ]
}
