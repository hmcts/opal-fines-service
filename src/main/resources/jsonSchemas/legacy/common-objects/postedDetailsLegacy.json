{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Posted Details Legacy",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "posted_date": {
      "type": "string",
      "format": "date"
    },
    "posted_by": {
      "type": "string",
      "maxLength": 20
    },
    "posted_by_name": {
      "type": "string",
      "maxLength": 100
    }
  },
  "required": [
    "posted_date"
  ]
}
