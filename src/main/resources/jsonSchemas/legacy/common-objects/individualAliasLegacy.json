{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Individual Alias Legacy",
  "type": "object",
  "properties": {
    "alias_id": { "type": "string" },
    "sequence_number": {
      "type": "number",
      "minimum": 1,
      "maximum": 5
    },
    "surname": { "type": "string" },
    "forenames": { "type": "string" }
  },
  "required": ["alias_id", "sequence_number"]
}
