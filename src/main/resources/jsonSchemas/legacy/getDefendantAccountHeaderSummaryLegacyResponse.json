{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Get Defendant Account Header Summary Legacy Response",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "version": { "type": "integer" },
    "defendant_account_id": { "type": "string" },
    "account_number": { "type": "string" },
    "defendant_party_id": { "type": "string" },
    "parent_guardian_party_id": { "type": "string" },
    "debtor_type": {
      "type": "string",
      "enum": ["Defendant", "Parent/Guardian"]
    },
    "is_youth": { "type": "boolean" },
    "account_status_reference": {
      "$ref": "./common-objects/accountStatusReferenceLegacy.json"
    },
    "account_type": {
      "type": "string",
      "enum": ["Fine", "Conditional Caution", "Confiscation", "Fixed Penalty"]
    },
    "prosecutor_case_reference": { "type": "string" },
    "fixed_penalty_ticket_number": { "type": "string" },
    "business_unit_summary": {
      "$ref": "./common-objects/businessUnitSummaryLegacy.json"
    },
    "payment_state_summary": {
      "$ref": "./common-objects/paymentStateSummaryLegacy.json"
    },
    "party_details": {
      "$ref": "./common-objects/partyDetailsLegacy.json"
    }
  },
  "required": [
    "defendant_account_id",
    "account_number",
    "defendant_party_id",
    "debtor_type",
    "is_youth",
    "account_status_reference",
    "account_type",
    "business_unit_summary",
    "payment_state_summary",
    "party_details"
  ]
}
