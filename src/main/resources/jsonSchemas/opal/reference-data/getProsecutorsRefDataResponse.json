{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://opal-fines-service/getProsecutorsRefDataResponse.json",
  "type": ["object", "null"],
  "properties": {
    "count": {
      "type": ["integer", "null"],
      "description": "The count of prosecutors returned in this response"
    },
    "ref-data": {
      "type": ["array", "null"],
      "items": {
        "$ref": "#/$defs/prosecutor"
      }
    }
  },
  "$defs": {
    "prosecutor": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "prosecutor_id": {
          "type": "integer",
          "format": "int16",
          "description": "Unique ID (DB Primary Key) for the prosecutor"
        },
        "name": {
          "type": "string",
          "description": "The name of the prosecutor"
        },
        "prosecutor_code": {
          "type": ["string", "null"],
          "description": "The code for the prosecutor"
        },
        "address_line_1": {
          "type": ["string", "null"],
          "maxLength": 60,
          "description": "The first line of the address of the prosecutor"
        },
        "address_line_2": {
          "type": ["string", "null"],
          "description": "The second line of the address of the prosecutor"
        },
        "address_line_3": {
          "type": ["string", "null"],
          "description": "The third line of the address of the prosecutor"
        },
        "address_line_4": {
          "type": ["string", "null"],
          "description": "The fourth line of the address of the prosecutor"
        },
        "address_line_5": {
          "type": ["string", "null"],
          "description": "The fifth line of the address of the prosecutor"
        },
        "postcode": {
          "type": ["string", "null"],
          "description": "The postcode of the prosecutor"
        },
        "end_date": {
          "type": ["string", "null"],
          "format": "date-time",
          "description": "The date up until which the prosecutor is active"
        }
      },
      "required": [
        "prosecutor_id",
        "name",
        "prosecutor_code",
        "address_line_1",
        "address_line_2",
        "address_line_3",
        "address_line_4",
        "address_line_5",
        "postcode",
        "end_date"
      ]
    }
  }
}
