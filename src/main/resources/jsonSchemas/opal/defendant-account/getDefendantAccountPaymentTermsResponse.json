{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "Get Defendant Account Payment Terms Response",
  "additionalProperties": false,
  "properties": {
    "payment_terms" : {
      "$ref": "../common-objects/paymentTerms.json"
    },
    "payment_card_last_requested": {
      "type": ["string", "null"],
      "format": "date"
    },
    "last_enforcement": {
      "type": ["string", "null"],
      "minLength": 1
    }
  },
  "required": ["payment_terms", "payment_card_last_requested", "last_enforcement"]
}

