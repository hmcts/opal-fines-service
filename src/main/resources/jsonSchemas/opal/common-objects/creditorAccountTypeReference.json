{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Creditor Account Type Reference",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "type": {
      "type": [
        "string",
        "null"
      ]
    },
    "display_name": {
      "type": [
        "string",
        "null"
      ],
      "enum": [
        "Minor Creditor",
        "Major Creditor",
        "Central Fund"
      ]
    }
  },
  "required": [
    "type",
    "display_name"
  ]
}
