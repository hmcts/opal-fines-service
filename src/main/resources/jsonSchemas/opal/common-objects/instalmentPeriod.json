{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Instalment Period",
  "type": ["object", "null"],
  "additionalProperties": false,
  "properties": {
    "instalment_period_code": {
      "type": "string",
      "enum": ["W", "M", "F"]
    },
    "instalment_period_display_name": {
      "type": "string",
      "enum": ["Weekly", "Monthly", "Fortnightly"]
    }
  },
  "required": ["instalment_period_code", "instalment_period_display_name"]
}
